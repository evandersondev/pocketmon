{
  "expo": {
    "name": "Pocketmon",
    "slug": "Pocketmon",
    "version": "1.1.0",
    "privacy": "public",
    "orientation": "portrait",
    "platforms": ["android"],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f1f2f6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.evandersondev.pocketmon",
      "adaptiveIcon": {
        "backgroundColor": "#FF6348"
      }
    }
  }
}
